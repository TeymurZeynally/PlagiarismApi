{
	"AllowedHosts": "*",
	"Secrets": {
		"MossUserId": 0,
		"GitHubToken": ""
	},
	"Cache": {
		"Expiration": "24:00:00"
	},
	"Serilog": {
		"MinimumLevel": "Debug",
		"WriteTo": [
			{
				"Name": "Console",
				"Args": {
					"outputTemplate": "{UtcTimestamp} [{Level}] {RequestId:l} {Scope} {Message:lj}{NewLine}{Exception}"
				}
			}
		]
	}
}
